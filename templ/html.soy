{namespace templ.mbi.web.html}

/*
 * Template for static site.
 */


/**
* @param canonical_url url
* @param is_raw raw or not
* @param assess_domain assess server domain
*/
{template .jsHead}
       <meta name="viewport" content="width=device-width, initial-scale=1">
{if $canonical_url}
  <link rel="canonical" href="{$canonical_url}"/>
{/if}
