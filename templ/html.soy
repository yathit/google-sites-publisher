    {/if}
{/template}


/**
* @param is_raw raw or not
*/
{template .jsBody}
    {if $is_raw}
        <script type="text/javascript" src="http://localhost/mbi-app/js/web/bootstrip-dev.js"></script>
    {/if}
{/template}


/**
* Head and body of HTML.
* @param title title.
*/
{template .innerHtml}
<head>
    <meta charset="utf-8"/>
    <title>{if $title}{$title} - {/if}MBInfo Wiki</title>
    {call .jsHead data="all"/}
</head>
<body>
  {call .body data="all"/}
  {call .jsBody data="all"/}
</body>
{/template}

/**
* Home page
* @param is_raw raw or not
*/
{template .index}

<!DOCTYPE html>
<html>
   {call .innerHtml data="all"/}
</html>

{/template}


/**
 * Body HTML.
 * @param breadcrumbPeer breadcrumb peer menu content.
 */
{template .sidebar}
<section class="sidebar">
  <div class="sidebar-top">
    <div class="logo" style="display: none">
      <a href="/">
      </a>
    </div>
    <div id="image-nav">
    </div>
  </div>
  <div class="sidebar-bottom">
     <div class="sidebar-bottom-scroller">
        <div role="navigation" id="breadcrumb-peer" class="menu-box">
        {if $breadcrumbPeer}
          {print $breadcrumbPeer |noAutoescape}
        {else}
          <ul class="breadcrumb-sidemenu">
            <li><a href="/topics/cytoskeleton-dynamics">Cytoskeleton Dynamics</a></li>
            <li><a href="/topics/synthesis">Synthesis</a></li>
            <li><a href="/topics/signaling">Signaling</a></li>
            <li><a href="/topics/modules">Modules</a></li>
            <li><a href="/topics/cellular-organization">Cellular Organization</a></li>
          </ul>
        {/if}
        </div>
        {call templ.mbi.app.topicDefinitionPanel/}
        <div id="toolbar">
        </div>

    </div>
  </div>
</section>
{/template}


/**
 * Body HTML.
 * @param content wiki content.
 * @param breadcrumb breadcrumb content.
 * @param breadcrumbPeer breadcrumb peer menu content.
 * @param date date value.
 */
{template .body}
<div id="body-content"><div>
  <div id="banner">
           <div class="left">
                      <a href="http://www.mechanobio.info/" id="home-link">

                     </a>
                 </div>
         <div class="right">
            <div class="top">
              <div class="login-bar">
                     <div>
                       <span id="user-name" style="display: none;"></span>
                     </div>
                     <div>
                       <a id="user-login">...</a>
                     </div>
                   </div>
             </div>
             <div class="bottom">
               <input type="text" placeholder="Search MBInfo" width="8em" id="search-input"/>
             </div>
         </div>
  </div>
  {call .sidebar data="all" /}
<div id="main-panel">
   <div id="main-panel-topbar">
      <div id="breadcrumb">
        {if $breadcrumb}
          {print $breadcrumb |noAutoescape}
        {/if}
      </div>
   </div>
   <div id="main-panel-content">
      <section class="content" id="section-content">
        <div id="content-main">
          {call templ.mbi.app.rightbar data="all"/}
          <div id="wiki-content" class="wiki-content">
            {if $content}
              {call templ.mbi.app.content data="all"/}
            {/if}
          </div>
        </div>
        {if $date}
          <div class="history">
            <div>
              <div class="left">Updated on: {$date}</div>
              <div class="center"></div>
              <div class="right"><a href="#" class="show-history">History</a></div>
            </div>
          </div>
        {/if}
      </section>
  </div>
</div>
{call .footer data="all" /}
</div></div>
{/template}

